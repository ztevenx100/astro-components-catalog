{
  "extends": "astro/tsconfigs/base",
  "include": [
    ".astro/types.d.ts", 
    "src/**/*"
  ],
  "exclude": [
    "dist",
    "node_modules",
    "**/*_backup*",
    "**/*.backup.*"
  ],
  "compilerOptions": {
    "strict": false,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true
  }
}
